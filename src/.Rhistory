a <- data.frame(id = c(1,2,1), value=c(1,2,3))
a %>%
distinct(id)
a %>%
distinct(id, .keep_all = T)
a <- data.frame(id = c(1,2,1), value=c(3,2,1))
a %>%
distinct(id, .keep_all = T)
a %>%
distinct(id, across(min(value)), .keep_all = T)
a %>%
distinct(id, .keep_all = T)
a %>%
arrange(value) %>%
distinct(id, .keep_all = T)
a %>%
arrange(value, desc) %>%
distinct(id, .keep_all = T)
